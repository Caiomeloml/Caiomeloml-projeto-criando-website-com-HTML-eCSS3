     body {
	 background: #f7f7f7;
	 font-family: verdana;
	 max-width: 900px;
	 margin: auto;
	 }
    
     .post_picture {
		 border: 2px solid #505050;
		 border-radius: 50%;
	 }

     #title, .subtitle, .post_title {
	      color: #505050;
      }
	 
	
	 
	 a{
		 text-decoration: line-through;
		 color: #505050;
	 }
	 
	 
	 .subtitle{
		 padding-left: 15px;
	 }
	 
	 
     .post_title {
	     font-size: 15px;
	     font-style: italic;
		 margin: 0;
		 margin-bottom: 15px;
		
     }

     .post {
        background: #FFF;
        padding: 20px;
		border: 3px solid #505050;
		margin: 10px;
		margin-bottom: 15px;
		border-radius: 10px;
		
     } 
	 .post_content {
		 margin: 0;
		 margin-bottom: 15px;
		 text-transform: capitalize;
		 text-align: justify;
	 }
	 
	 .post_image {
		 margin-bottom: 10px;
		 border-radius: 10px;
		 max-width: 900px;
	     margin: auto;
	 }

	 .contact_list {
	     list-style-type: none;
		 padding-left: 15px;
		 
	 }
	 
	 .contact_list li a {
		 text-decoration: none;
		 color: blue;
	 }
	 